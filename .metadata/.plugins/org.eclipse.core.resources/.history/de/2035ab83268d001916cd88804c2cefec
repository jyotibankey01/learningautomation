public class test2 {

	@Test
	public void test1(){
		System.out.println("test1");
	}
	@Test
	public void test2(){
		System.out.println("test2");
	}
	
	@BeforeMethod
	public void precondition() {
		System.out.println("beforemethod");
	}
	
	@AfterMethod
	public void postcondition() {
		System.out.println("aftermenthod");
	}
}
